@model Kemergency.Models.Rental


<div class="modal fade" id="add-contact" tabindex="-1" role="dialog" aria-labelledby="addContactLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="addContactLabel">Add contact</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-4">
                        <form asp-action="_New">
                            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                            <input type="hidden" asp-for="Id" />
                            <div class="form-group">
                                <label asp-for="MyusersId" class="control-label"></label>
                                <input asp-for="MyusersId" class="form-control" />
                                <span asp-validation-for="MyusersId" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="HospitalId" class="control-label"></label>
                                <input asp-for="HospitalId" class="form-control" />
                                <span asp-validation-for="HospitalId" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Ambulanc_SchedualId" class="control-label"></label>
                                <input asp-for="Ambulanc_SchedualId" class="form-control" />
                                <span asp-validation-for="Ambulanc_SchedualId" class="text-danger"></span>
                            </div>
                            <div class="form-group form-check">
                                <label class="form-check-label">
                                    <input class="form-check-input" asp-for="Isbooking_Confirmed" /> @Html.DisplayNameFor(model => model.Isbooking_Confirmed)
                                </label>
                            </div>
                            <div class="form-group">
                                <label asp-for="DateRented" class="control-label"></label>
                                <input asp-for="DateRented" class="form-control" />
                                <span asp-validation-for="DateRented" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="DateReturned" class="control-label"></label>
                                <input asp-for="DateReturned" class="form-control" />
                                <span asp-validation-for="DateReturned" class="text-danger"></span>
                            </div>
                            <div class="form-group">
                                <input type="submit" value="Save" class="btn btn-primary" />
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary">Save</button>
            </div>
        </div>
    </div>
</div>
data-url="@Url.Action("New", "NewRentals", 1)


<div>
    <a asp-action="Index">Back to List</a>
</div>

